	<dependency>
		<groupId>org.springframework</groupId>
		<artifactId>spring-webmvc-portlet</artifactId>
		<version>${org.springframework.version}</version>
	</dependency>
	
	<!-- spring-instrument 提供一些类级工具支持和ClassLoader级的实现,用于服务器 -->
